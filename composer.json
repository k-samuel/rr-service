{
    "name": "k-samuel/rr-service",
    "version": "1.0.0-dev",
    "type": "project",
    "description": "RoadRunner Service",
    "keywords": ["php"],
    "homepage": "https://github.com/k-samuel/rr-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/k-samuel/rr-service/issues",
        "source": "https://github.com/k-samuel/rr-service"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php":">=7.4.0",
        "spiral/roadrunner": "^2.0",
        "laminas/laminas-db": "^2.10",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0",
        "php-amqplib/php-amqplib": "^3.0",
        "laminas/laminas-httphandlerrunner": "^1.3",
        "psr/log": "^1.1",
        "psr/container": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "foolz/sphinxql-query-builder": "^3.0",
        "dvelum/cache": "^4.0",
        "dvelum/db": "^4.0"
    },
    "autoload": {
      "psr-4": {
        "KSamuel\\RrService\\": "src/",
        "App\\Example\\": "services/example/src"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "codacy/coverage": "dev-master",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
